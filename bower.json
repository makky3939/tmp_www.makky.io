{
  "name": "Makky.io",
  "version": "0.0.0",
  "authors": [
    "Makky"
  ],
  "moduleType": [
    "node"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "directory": "bower_components",
  "devDependencies": {
    "bootstrap": "~3.1.1",
    "jquery": "~2.1.0",
    "font-awesome": "~4.0.3",
